{
  "name": "travelaroundtheworld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "dotenv -e backend/.env -- nodemon --watch backend/src/apollo --ext ts --exec tsx backend/src/apollo/server.ts",
    "client": "dotenv -e frontend/.env -- vite --config frontend/vite.config.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build frontend",
    "build:server": "tsc -p backend/tsconfig.json"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@prisma/client": "^6.11.1",
    "@types/node": "^24.0.12",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^8.0.0",
    "emoji-flags": "^1.3.0",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "json-server": "^1.0.0-beta.3",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "nodemon": "^3.1.10",
    "react": "^19.0.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-router": "^7.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/emoji-flags": "^1.3.3",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.17",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prisma": "^6.11.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
